# Example environment variables (copy to .env.local and fill values)

# Google OAuth (Google Cloud Console → Credentials → OAuth client ID)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Auth.js / NextAuth
NEXTAUTH_SECRET=dev-secret-change-me
NEXTAUTH_URL=http://localhost:3000

# Database (Postgres) - Docker local development
DATABASE_URL=postgresql://bombay:password@127.0.0.1:5432/bombay_dev?sslmode=disable

# Providers (server-only)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Seeding (optional)
SEED_USER_EMAIL=dev@example.com

# Optional rate limiting
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=

# Better Stack logs
BETTERSTACK_CONNECT_HOST=
BETTERSTACK_CONNECT_USER=
BETTERSTACK_CONNECT_PASS=
BETTERSTACK_TABLE_PREFIX=
